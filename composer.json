{
    "name": "makuguren/laravel-livewire-calendar",
    "description": "Laravel Livewire calendar component",
    "keywords": [
        "makuguren",
        "laravel-livewire-calendar"
    ],
    "homepage": "https://github.com/makuguren/laravel-livewire-calendar",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Mark Glen Miguel",
            "email": "markgmiguel@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2|^8.3",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "livewire/livewire": "^2.0||^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Makuguren\\LivewireCalendar\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Makuguren\\LivewireCalendar\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Makuguren\\LivewireCalendar\\LivewireCalendarServiceProvider"
            ],
            "aliases": {
                "LivewireCalendar": "Makuguren\\LivewireCalendar\\LivewireCalendarFacade"
            }
        }
    }
}
